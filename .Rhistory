oldpar <- par(mfrow = dims, mai = par('mai')/5)
on.exit(par(oldpar))
for (i in 1:length(witcher_palettes)) {
image(matrix(1:50, ncol = 1), col = witcher(n=255,option = witcher_palette_names()[[i]]), axes = FALSE)
text(0.5,0.5,witcher_palette_names()[[i]], cex=1.5)
}
